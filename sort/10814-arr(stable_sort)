#include <iostream>
// stable sort의 경우 메모리를 sort보다 더 많이 차지하는 경향이 존재함. 그러나, 메모리 용량에 따라 알아서 알고리즘을 조절함.

#include <algorithm>
#include <vector>
#include <string>

using namespace std;
typedef long long ll;

struct member{
    int age;
    string name;
};

bool compare(const member &a, const member &b){
    return a.age < b.age;
}

member arr[100001];

int main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    
    int n;
    cin >> n;
    
    for(int i = 0; i < n; i ++){
        cin >> arr[i].age >> arr[i].name;
    }
    
    stable_sort(arr, arr + n, compare);
    
    for(int i = 0; i < n; i++){
        cout << arr[i].age << " " << arr[i].name << '\n';
    }

    return 0;
}
